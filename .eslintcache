                                       </button>\r\n                                                <button\r\n                                                    onClick={() => selectDocType(topic._id, 'attachments')}\r\n                                                    disabled={topic.attachments.length === 0}>\r\n                                                    {t(\"Documents\")}\r\n                                                </button>\r\n                                            </div>\r\n                                        </div>\r\n                                        <div className='topic__date'>\r\n                                            {moment(topic.createdAt).format(\"DD.MM.YYYY\")}\r\n                                        </div>\r\n                                    </div>\r\n                                    {topic._id === topicId && <Materials materialsArray={topic[docType]} lang={lang} />}\r\n                                </>\r\n                            ))}\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </>\r\n    )\r\n}\r\n\r\nconst Materials = ({materialsArray, lang}) => {\r\n    return (\r\n        <div className='topic__materials'>\r\n            {materialsArray.length > 0 && materialsArray.map(material =>\r\n                material.fileLink[lang] ? <Material material={material} lang={lang} key={material._id} />\r\n                : <div dangerouslySetInnerHTML={{__html: material.description[lang]}}/>\r\n            )}\r\n        </div>\r\n    )\r\n}\r\n\r\nconst Material = ({material, lang}) => {\r\n    const extention = material.fileLink[lang].split('.').pop();\r\n    switch(extention) {\r\n        case 'docx':\r\n            return (\r\n                <a href={material.fileLink[lang]} target='_blanck'>\r\n                    <div className='material-doc'>\r\n                        <div><WordIcon /></div>\r\n                        <div>{material.name[lang]}</div>\r\n                    </div>\r\n                </a>\r\n                \r\n            );\r\n        case 'pdf':\r\n            return (\r\n                <a href={material.fileLink[lang]} target='_blanck'>\r\n                    <div className='material-doc'>\r\n                        <div><PDFIcon /></div>\r\n                        <div>{material.name[lang]}</div>\r\n                    </div>\r\n                </a>\r\n            );\r\n        case 'pptx':\r\n            return (\r\n                <a href={material.fileLink[lang]} target='_blanck'>\r\n                    <div className='material-doc'>\r\n                        <div><PowerPointIcon /></div>\r\n                        <div>{material.name[lang]}</div>\r\n                    </div>\r\n                </a>\r\n            );\r\n        default:\r\n            return (\r\n                <div className='material'>\r\n                    <iframe src={`${material.fileLink[lang]}&embedded=true`} width='100%' title={material.name[lang]} />\r\n                    {material.name[lang]}\r\n                </div>\r\n            );\r\n    }\r\n}\r\n\r\nexport default Recources;","R:\\Work\\freelance\\bhrc-frontend\\src\\history.js",[],"R:\\Work\\freelance\\bhrc-frontend\\src\\components\\Pages\\Activities\\Activity.jsx",[],"R:\\Work\\freelance\\bhrc-frontend\\src\\redux\\reducers\\recourceReducer.js",[],"R:\\Work\\freelance\\bhrc-frontend\\src\\redux\\actions\\resource-action.js",[],"R:\\Work\\freelance\\bhrc-frontend\\src\\components\\Elements\\NewsHeader\\NewsHeader.jsx",[],"R:\\Work\\freelance\\bhrc-frontend\\src\\components\\Pages\\Activities\\ActivitiesMain.jsx",["191"],{"ruleId":"192","replacedBy":"193"},{"ruleId":"194","replacedBy":"195"},{"ruleId":"196","severity":1,"message":"197","line":5,"column":1,"nodeType":"198","endLine":15,"endColumn":2},{"ruleId":"199","severity":1,"message":"200","line":8,"column":13,"nodeType":"201","messageId":"202","endLine":8,"endColumn"